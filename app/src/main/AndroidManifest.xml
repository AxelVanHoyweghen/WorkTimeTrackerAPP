<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worktimetrackerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new"
        android:label="WorkTimeTracker"
        android:roundIcon="@mipmap/ic_launcher_new_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.worktimetrackerapp.EntryPoint"
            android:label="WTT"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WTTApplication" />
        <activity android:name=".GUI_Interfaces.SignIn_Controller" />
        <activity android:name=".GUI_Interfaces.SignUp_Controller" />

    </application>

</manifest>